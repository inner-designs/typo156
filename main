#include <iostream>
#include <string>
#include <cassert>
using namespace std;

bool isIdentical(const string dict[], int n, string word){
    for (int i = 0; i < n; i++){
        if(dict[i] == word)
            return true;
    }
    return false;
}

int scoreTypo(const string dict[], int n, string word)
{
    if (n <= 0)
        return -1;
        
    if (isIdentical(dict, n, word))
        return 0;
    
    return 2;
    
}

int main()
{
    string dict1[7] = { "januarty", "february", "march", "april", "may", "june", "july"};
    
    assert(scoreTypo(dict1, 7, "april") == 0);
}
